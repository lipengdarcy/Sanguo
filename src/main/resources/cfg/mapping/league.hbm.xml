<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.darcy.sanguo.union">
	<class name="League" table="league">
		<id name="id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="string" />
		<property name="level" column="level" type="int" />
		<property name="rank" column="rank" type="int" />
		<property name="buildValue" column="build_value" type="int" />
		<property name="notice" column="notice" type="string" />
		<property name="memberLimit" column="member_limit" type="int" />
		<property name="leader" column="leader" type="int" />
		<property name="shopLevel" column="shop_level" type="int" />
		<property name="goodsLevel" column="goods_level" type="int" />
		<property name="bossFacilityLevel" column="boss_facility_level" type="int" />
		<property name="info" column="info" type="org.darcy.sanguo.usertype.LeagueInfoUserType" />
	</class>
</hibernate-mapping>
